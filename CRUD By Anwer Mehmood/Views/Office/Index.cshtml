
@model IEnumerable<CRUD_By_Anwer_Mehmood.Models.Office>
@{
    ViewData["Title"] = "Index";
}


<div><h1>List of Employee</h1></div>
<div> <a class="btn btn-success" asp-action="Create">Create</a></div>


<table class="table table-bordered table-hover table-light">
    <thead class="thead-light">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Number</th>
            <th>Action</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Address</td>
                <td>@item.Number</td>
                <td>
                    <a asp-action="Edit" asp-route-Id="@item.Id" class="btn btn-info">Edit</a>||
                    <a asp-action="Delete" asp-route-Id="@item.Id" class="btn btn-danger">Delete</a>
                </td>
            </tr>

        }

    </tbody>
</table>